{
    "version": 2,
    "functions": {
      "api/*.js": {
        "maxDuration": 10
      }
    },
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ]
  }
  